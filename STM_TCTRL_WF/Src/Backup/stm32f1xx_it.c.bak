  /* USER CODE END EXTI3_IRQn 1 */
}

/**
  * @brief This function handles EXTI line[9:5] interrupts.
  */
void EXTI9_5_IRQHandler(void)
{
  /* USER CODE BEGIN EXTI9_5_IRQn 0 */
//NRF IRQ
	if(EXTI->PR & GPIO_PIN_7)//IRQ2
  {
		#ifdef NRF24_SPI_PORT2
    NRF24_IRQ_Callback();
		#endif
  }
	if(EXTI->PR & GPIO_PIN_8) //IRQ1
  {
    #ifdef NRF24_SPI_PORT1
    NRF24_IRQ_Callback();
		#endif
  }
	

  /* USER CODE END EXTI9_5_IRQn 0 */
  HAL_GPIO_EXTI_IRQHandler(GPIO_PIN_7);
  HAL_GPIO_EXTI_IRQHandler(GPIO_PIN_8);
  /* USER CODE BEGIN EXTI9_5_IRQn 1 */

  /* USER CODE END EXTI9_5_IRQn 1 */
}

/**
  * @brief This function handles TIM1 update interrupt.
  */
void TIM1_UP_IRQHandler(void)
{
  /* USER CODE BEGIN TIM1_UP_IRQn 0 */
  SysCnt.temp_update++;
  SysCnt.timeout++;
	SysCnt.temp_ctrl++;
	
	packet_handler_timer++;

	//printf("timeout: %d\r\n", SysCnt.timeout);
	//printf("temp_update: %d\r\n", SysCnt.temp_update);
	if (SysState.error_code)
			SysCnt.error_check++;

	if (SysState.ss_blink_f) {					
		SysCnt.blink++;
			if (SysCnt.blink >= SS_BLINK_TIME) {
					SysCnt.blink = 0;
					SS_OE_Toggle();
			}
	}
	if(drv_m1.run_f){
		SysCnt.drv_move++;
		
		if(HAL_GPIO_ReadPin(PHOTOSENSOR_GPIO_Port, PHOTOSENSOR_Pin)!=drv_m1.photosensor_state){
			drv_m1.photosensor_state^=1;
			SysCnt.drv_move=0;
			
			if(drv_m1.direction){
				drv_m1.pos++;
			}else{
				drv_m1.pos--;
			}
			if(drv_m1.pos==drv_m1.dest_pos){
				DRV_STOP();
			}
			
		}		
		if(SysCnt.drv_move>DRIVE_ROTATION_TIMEOUT){			
			DRV_STOP();
			drv_m1.fail_f=1;	
			DRV_STOP();
			if (drv_m1.direction) {
				SysState.error_code |= DRV_CLOSE_ERROR;
			} else{
				SysState.error_code |= DRV_OPEN_ERROR;
			}
          
			printf("DRV MOVE FAIL\r\n");

    }
	}
	


  /* USER CODE END TIM1_UP_IRQn 0 */
  HAL_TIM_IRQHandler(&htim1);
  /* USER CODE BEGIN TIM1_UP_IRQn 1 */

  /* USER CODE END TIM1_UP_IRQn 1 */
}

